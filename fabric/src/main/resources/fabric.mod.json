{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "accessWidener" : "${mod_id}.accesswidener",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://minecraft.curseforge.com/projects/ping",
        "sources": "https://github.com/dmillerw/Ping/issues"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.girafi.ping.Ping"
        ],
        "client": [
            "com.girafi.ping.client.ClientHandler"
        ]
    },
    "depends": {
        "fabricloader": ">=0.15.3",
        "fabric-api": "*",
        "minecraft": "~1.20.4",
        "java": ">=17",
        "fabric-key-binding-api-v1": "*",
        "forgeconfigapiport": ">=20.4.1"
    },
    "suggests": {
        "another-mod": "*"
    }
}